{
    "webdav": {
        "url": "https://your-webdav-server.com",
        "username": "your-username",
        "password": "your-password",
        "remotePath": "/SillyTavern-Backup"
    },
    "watchDir": "../data",
    "watchConfigDir": "../config",
    "debounceMs": 2000,
    "initialSync": true,
    "syncDelete": false,
    "verbose": false,
    "statsInterval": 300,
    "restoreConcurrency": 50,
    "ignorePatterns": [
        "_cache",
        "_webpack",
        "thumbnails",
        ".tmp",
        ".temp",
        "node_modules"
    ]
}
